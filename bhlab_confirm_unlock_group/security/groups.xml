<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
	    <record id="group_allow_confirm_unlock_account" model="res.groups">
	        <field name="name">Allow Confirm &amp; Unlock account's</field>
	        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
	        <field name="comment">Users in this group can confirm &amp; unlock Accounts for further editing.</field>
	    </record>
		<record id="group_allow_confirm_picking" model="res.groups">
	        <field name="name">Allow Confirm, cancel picking and reception </field>
	        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
	        <field name="comment">Users in this group can Confirm, cancel picking and reception.</field>
	    </record>
		<record id="group_allow_confirm_out" model="res.groups">
	        <field name="name">Allow Confirm, cancel outs, internal transfer</field>
	        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
	        <field name="comment">Users in this group can Confirm, cancel outs, internal transfer and retourns.</field>
	    </record>
	</data>
</odoo>